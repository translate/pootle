#: static/js/common.js:89
msgid "Collapse details"
msgstr "Riduci dettagli"

#: static/js/common.js:123
msgid "Are you sure you want to continue?"
msgstr "Sei sicuro di voler continuare?"

#: static/js/common.js:124
msgid "This operation cannot be undone."
msgstr "Questa azione non può essere annullata."

#: static/js/editor.js:405 static/js/editor.js.c:1475
msgid "No results."
msgstr "Nessun risultato."

#. Translators: Permalink to the current unit in the editor.
#. The first '%s' is the permalink URL.
#. The second '%s' is the unit number.
#: static/js/editor.js:722
#, c-format
msgid "<a href=\"%s\">String %s</a>"
msgstr "<a href=\"%s\">Stringa %s</a>"

#: static/js/editor.js:776
msgid "Error while connecting to the server"
msgstr "Errore durante la connessione al server"

#: static/js/editor.js:778
msgid "Server error"
msgstr "Errore del server"

#: static/js/editor.js:780
msgid "The server seems down. Try again later."
msgstr "Il server sembra non rispondere. Riprova più tardi."

#: static/js/editor.js:788
msgid "Unknown error"
msgstr "Errore sconosciuto"

#: static/js/editor.js:1324
msgid "Congratulations, you walked through all strings."
msgstr "Congratulazioni, hai girato per tutte le stringhe."

#: static/js/editor.js:1326 static/js/editor.js.c:1350
msgid "Return to the overview page."
msgstr "Ritorna alla pagina di riepilogo."

#: static/js/editor.js:1345
msgid "You reached the beginning of the list"
msgstr "Hai raggiunto l'inizio della lista"

#: static/js/editor.js:1348
msgid "You reached the end of the list."
msgstr "Hai raggiunto la fine della lista."

#. Translators: This is the quality match percentage of a TM result.
#. '%s' will be replaced by a number, and you should keep the extra
#. '%' symbol to denote a percentage is being used.
#: static/js/editor.js:1742
#, c-format
msgid "%s% match"
msgstr "%s% corrispondente"

