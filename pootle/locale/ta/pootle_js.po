msgid "More"
msgstr "மேலும்"

#. Translators: from pootle/apps/pootle_store/templates/unit/xhr-edit-ctx.html
#: static/js/strings.js:10
msgid "Hide"
msgstr "மறை"

#. Translators: from pootle/apps/pootle_store/templates/unit/xhr-edit-ctx.html
#: static/js/strings.js:13
msgid "Less"
msgstr "குறைவு"

#. Translators: from pootle/apps/pootle_store/templates/unit/xhr-edit-ctx.html
#: static/js/strings.js:16
msgid "Show"
msgstr "காட்டு"

